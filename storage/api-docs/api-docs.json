                    "200": {
                        "description": "List of business cards",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/BusinessCard"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Business Cards"
                ],
                "summary": "Store a newly created business card",
                "description": "Store a newly created business card.",
                "operationId": "0adb369481048e09a76d1e31c48885b3",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
